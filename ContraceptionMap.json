{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",

  "description": "Map of Europe using topojson file obtained from deldersveld public GitHub repository.",

    "title": {
      "text": "Rating of Access to Contraception in Europe",
      "subtitle": "Based on access to means of contraception, family planning advice and online information about contraception. Data source: Statista",
      "subtitleFontStyle": "italic",
      "subtitleFontSize": 10,
      "anchor": "start",
      "color": "black"
    },

  "width": 500,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/deldersveld/topojson/master/continents/europe.json",
    "format": {"type": "topojson", "feature": "continent_Europe_subunits"}
  },
  "transform": [
    {
      "lookup": "properties.geounit",
      "from": {
        "data": {
          "url": "https://raw.githubusercontent.com/htylerstreet/htylerstreet.github.io/main/AccessToContraception1.csv"
        },
        "key": "Country",
        "fields": ["Rating"]
      }
    }
  ],
  "projection": {"type": "equalEarth"},
  "mark": "geoshape",
  "encoding": {
    "color": {"field": "Rating", "title": "Rating (%)", "type": "quantitative", "scale": {"scheme": "yellowgreenblue"}},
    "tooltip": [
      {"field": "properties.geounit", "type": "nominal", "title": "Country"},
      {"field": "Rating", "type": "quantitative", "title": "Rating (%)"}
    ]
  }
}